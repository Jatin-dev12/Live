</div>

<% script=`<script src="/js/table-search.js">
    </script>
    <script>
        // Note: Using server-side search instead of client-side table search

        // Form type filter functionality
        $('#formTypeFilter').on('change', function() {
            const selectedType = $(this).val();
            const currentUrl = new URL(window.location);
            
            // Reset to page 1 when changing filter
            currentUrl.searchParams.delete('page');
            
            if (selectedType === 'all') {
                currentUrl.searchParams.delete('formType');
            } else {
                currentUrl.searchParams.set('formType', selectedType);
            }
            
            window.location.href = currentUrl.toString();
        });

        // Export CSV functionality
        $('#exportCsvBtn').on('click', function () {
            const currentFormType = $(this).data('current-form-type');
            const currentSearch = $(this).data('current-search');
            let exportUrl = '/leads/export-csv';
            const params = [];
            
            if (currentFormType !== 'all') {
                params.push('formType=' + currentFormType);
            }
            if (currentSearch) {
                params.push('search=' + encodeURIComponent(currentSearch));
            }
            
            if (params.length > 0) {
                exportUrl += '?' + params.join('&');
            }
            
            window.location.href = exportUrl;
        });

        // View Lead Details
        $('.view-lead-btn').on('click', function () {
            const leadData = JSON.parse($(this).attr('data-lead-data'));

            // Format and populate date
            if (leadData.createdAt) {
                const date = new Date(leadData.createdAt);
                $('#modal-date').text(date.toLocaleDateString('en-US', {
                    year: 'numeric',
                    month: 'short',
                    day: 'numeric'
                }));
            } else {
                $('#modal-date').text('N/A');
            }

            // Populate form type
            const formType = leadData.formType || 'Unknown';
            $('#modal-formType').text(formType.charAt(0).toUpperCase() + formType.slice(1));

            // Populate name based on form type
            let displayName = 'N/A';
            if (leadData.fullName) displayName = leadData.fullName;
            else if (leadData.name) displayName = leadData.name;
            else if (leadData.complainantName) displayName = leadData.complainantName;
            else if (leadData.firstName && leadData.lastName) displayName = leadData.firstName + ' ' + leadData.lastName;
            $('#modal-fullName').text(displayName);

            // Populate email based on form type
            const email = leadData.email || leadData.complainantEmail;
            const emailElement = $('#modal-email');
            if (email && email !== 'N/A') {
                emailElement.text(email);
                emailElement.attr('href', 'mailto:' + email);
                emailElement.css('pointer-events', 'auto');
                emailElement.removeClass('text-secondary-light').addClass('text-primary-600');
            } else {
                emailElement.text('N/A');
                emailElement.attr('href', '#');
                emailElement.css('pointer-events', 'none');
                emailElement.removeClass('text-primary-600').addClass('text-secondary-light');
            }

            // Make phone clickable
            const phoneElement = $('#modal-phone');
            if (leadData.phone && leadData.phone !== 'N/A') {
                phoneElement.text(leadData.phone);
                phoneElement.attr('href', 'tel:' + leadData.phone);
                phoneElement.css('pointer-events', 'auto');
                phoneElement.removeClass('text-secondary-light').addClass('text-primary-600');
            } else {
                phoneElement.text('N/A');
                phoneElement.attr('href', '#');
                phoneElement.css('pointer-events', 'none');
                phoneElement.removeClass('text-primary-600').addClass('text-secondary-light');
            }

            // Populate subject based on form type
            let subject = 'N/A';
            switch(leadData.formType) {
                case 'contact':
                    subject = leadData.selectOne || leadData.interestedIn || 'Contact Inquiry';
                    break;
                case 'suggestion':
                    subject = 'Suggestion Box';
                    break;
                case 'accessibility':
                    subject = 'Accessibility Feedback';
                    break;
                case 'grievance':
                    subject = 'Grievance Report';
                    break;
                case 'complaint':
                    subject = leadData.complaintNature || 'Formal Complaint';
                    break;
                default:
                    subject = leadData.interestedIn || 'General Inquiry';
            }
            $('#modal-subject').text(subject);

            // Populate form-specific fields
            let formSpecificHtml = '';
            switch(leadData.formType) {
                case 'contact':
                    if (leadData.cityCountryTimezone) {
                        formSpecificHtml += '<div class="d-flex align-items-center gap-3 mb-2">' +
                            '<iconify-icon icon="mdi:map-marker" class="text-info-600" style="font-size: 28px;"></iconify-icon>' +
                            '<label class="form-label fw-bold mb-0" style="font-size: 18px;">Location</label>' +
                            '</div>' +
                            '<p class="text-secondary-light ms-5 mb-3" style="font-size: 17px; font-weight: 600;">' + leadData.cityCountryTimezone + '</p>';
                    }
                    if (leadData.areasOfInterest) {
                        formSpecificHtml += '<div class="d-flex align-items-center gap-3 mb-2">' +
                            '<iconify-icon icon="mdi:star" class="text-warning-600" style="font-size: 28px;"></iconify-icon>' +
                            '<label class="form-label fw-bold mb-0" style="font-size: 18px;">Areas of Interest</label>' +
                            '</div>' +
                            '<p class="text-secondary-light ms-5 mb-3" style="font-size: 17px; font-weight: 600;">' + leadData.areasOfInterest + '</p>';
                    }
                    break;
                case 'suggestion':
                    if (leadData.mood) {
                        formSpecificHtml += '<div class="d-flex align-items-center gap-3 mb-2">' +
                            '<iconify-icon icon="mdi:emoticon" class="text-success-600" style="font-size: 28px;"></iconify-icon>' +
                            '<label class="form-label fw-bold mb-0" style="font-size: 18px;">Mood</label>' +
                            '</div>' +
                            '<p class="text-secondary-light ms-5 mb-3" style="font-size: 17px; font-weight: 600;">' + leadData.mood + '</p>';
                    }
                    break;
                case 'accessibility':
                    formSpecificHtml += '<div class="d-flex align-items-center gap-3 mb-2">' +
                        '<iconify-icon icon="mdi:wheelchair-accessibility" class="text-primary-600" style="font-size: 28px;"></iconify-icon>' +
                        '<label class="form-label fw-bold mb-0" style="font-size: 18px;">Conference Details</label>' +
                        '</div>' +
                        '<div class="ms-5 mb-3">' +
                        '<p class="mb-1"><strong>Attended:</strong> ' + (leadData.attendedConference || 'N/A') + '</p>' +
                        '<p class="mb-1"><strong>Had Accessibility Needs:</strong> ' + (leadData.hadAccessibilityNeeds || 'N/A') + '</p>' +
                        '<p class="mb-1"><strong>Needs Met:</strong> ' + (leadData.needsMet || 'N/A') + '</p>' +
                        '<p class="mb-1"><strong>Venue Rating:</strong> ' + (leadData.venueRating || 'N/A') + '/5</p>' +
                        '<p class="mb-0"><strong>Resources Rating:</strong> ' + (leadData.resourcesRating || 'N/A') + '/5</p>' +
                        '</div>';
                    break;
                case 'grievance':
                    formSpecificHtml += '<div class="d-flex align-items-center gap-3 mb-2">' +
                        '<iconify-icon icon="mdi:shield-alert" class="text-warning-600" style="font-size: 28px;"></iconify-icon>' +
                        '<label class="form-label fw-bold mb-0" style="font-size: 18px;">Grievance Details</label>' +
                        '</div>' +
                        '<div class="ms-5 mb-3">' +
                        '<p class="mb-1"><strong>Visibility:</strong> ' + (leadData.grievanceVisibility || 'N/A') + '</p>' +
                        '<p class="mb-1"><strong>Share With:</strong> ' + (leadData.shareWith || 'N/A') + '</p>';
                    if (leadData.incidentDate) {
                        formSpecificHtml += '<p class="mb-1"><strong>Incident Date:</strong> ' + new Date(leadData.incidentDate).toLocaleDateString() + '</p>';
                    }
                    if (leadData.witnessName) {
                        formSpecificHtml += '<p class="mb-0"><strong>Witness:</strong> ' + leadData.witnessName + '</p>';
                    }
                    formSpecificHtml += '</div>';
                    break;
                case 'complaint':
                    formSpecificHtml += '<div class="d-flex align-items-center gap-3 mb-2">' +
                        '<iconify-icon icon="mdi:gavel" class="text-danger-600" style="font-size: 28px;"></iconify-icon>' +
                        '<label class="form-label fw-bold mb-0" style="font-size: 18px;">Complaint Details</label>' +
                        '</div>' +
                        '<div class="ms-5 mb-3">' +
                        '<p class="mb-1"><strong>Respondent:</strong> ' + (leadData.respondentName || 'N/A') + '</p>' +
                        '<p class="mb-1"><strong>Nature:</strong> ' + (leadData.complaintNature || 'N/A') + '</p>';
                    if (leadData.incidentDate) {
                        formSpecificHtml += '<p class="mb-1"><strong>Incident Date:</strong> ' + new Date(leadData.incidentDate).toLocaleDateString() + '</p>';
                    }
                    formSpecificHtml += '<p class="mb-0"><strong>Acknowledged:</strong> ' + (leadData.acknowledgement ? 'Yes' : 'No') + '</p>' +
                        '</div>';
                    break;
            }
            $('#modal-formSpecificFields').html(formSpecificHtml);

            // Populate notes/message based on form type
            let message = 'No message available';
            if (leadData.notes) message = leadData.notes;
            else if (leadData.suggestion) message = leadData.suggestion;
            else if (leadData.incidentDescription) message = leadData.incidentDescription;
            else if (leadData.detailedDescription) message = leadData.detailedDescription;
            
            $('#modal-notes').text(message);

            // Show modal
            $('#leadDetailsModal').modal('show');
        });
    </script>
