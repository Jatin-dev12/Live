<div class="card p-0 radius-12">
    <div
        class="card-header border-bottom bg-base py-16 px-24 d-flex align-items-center flex-wrap gap-3 justify-content-between">
        <div class="d-flex align-items-center flex-wrap gap-3">
            <form class="navbar-search" method="GET" action="/leads/leads-management">
                <input type="text" name="search" class="bg-base h-40-px w-auto" placeholder="Search leads..." value="<%= search %>">
                <% if (currentFormType !== 'all') { %>
                    <input type="hidden" name="formType" value="<%= currentFormType %>">
                <% } %>
                <button type="submit" class="search-btn">
                    <iconify-icon icon="ion:search-outline" class="icon"></iconify-icon>
                </button>
            </form>
            <div class="d-flex align-items-center gap-2">
                <label class="form-label mb-0 fw-semibold text-primary-light">Filter by Type:</label>
                <select id="formTypeFilter" class="form-select form-select-sm" style="width: auto; min-width: 150px;">
                    <option value="all" <%= currentFormType === 'all' ? 'selected' : '' %>>All Forms (<%= totalLeads %>)</option>
                    <option value="contact" <%= currentFormType === 'contact' ? 'selected' : '' %>>
                        Contact (<%= formTypeStats.find(stat => stat._id === 'contact')?.count || 0 %>)
                    </option>
                    <option value="suggestion" <%= currentFormType === 'suggestion' ? 'selected' : '' %>>
                        Suggestion (<%= formTypeStats.find(stat => stat._id === 'suggestion')?.count || 0 %>)
                    </option>
                    <option value="accessibility" <%= currentFormType === 'accessibility' ? 'selected' : '' %>>
                        Accessibility (<%= formTypeStats.find(stat => stat._id === 'accessibility')?.count || 0 %>)
                    </option>
                    <option value="grievance" <%= currentFormType === 'grievance' ? 'selected' : '' %>>
                        Grievance (<%= formTypeStats.find(stat => stat._id === 'grievance')?.count || 0 %>)
                    </option>
                    <option value="complaint" <%= currentFormType === 'complaint' ? 'selected' : '' %>>
                        Complaint (<%= formTypeStats.find(stat => stat._id === 'complaint')?.count || 0 %>)
                    </option>
                </select>
            </div>
        </div>
        <div class="d-flex align-items-center gap-2">
            <button id="exportCsvBtn"
                class="btn btn-sm btn-primary d-flex align-items-center gap-1"
                data-current-form-type="<%= currentFormType %>"
                data-current-search="<%= search %>">
                <iconify-icon icon="material-symbols:download" class="icon text-xl line-height-1"></iconify-icon>
                Export CSV
            </button>
            <!-- <a href="/leads/add-leads" class="btn btn-primary text-sm btn-sm px-12 py-12 radius-8 d-flex align-items-center gap-2">
                <iconify-icon icon="ic:baseline-plus" class="icon text-xl line-height-1"></iconify-icon>
                Add New Leads
            </a> -->
        </div>
    </div>
    <div class="card-body p-24">
        <div class="table-responsive scroll-sm">
            <!-- <table id="dataTable" class="table bordered-table sm-table mb-0"> -->
            <table id="dataTable" class="table custom-table">
                <thead>
                    <tr>
                        <th scope="col">Date</th>
                        <th scope="col">Form Type</th>
                        <th scope="col">Name</th>
                        <th scope="col">Email</th>
                        <th scope="col">Phone</th>
                        <th scope="col">Subject</th>
                        <th scope="col" class="text-center">View Details</th>
                    </tr>
                </thead>
                <tbody>
                    <% if (leads && leads.length > 0) { %>
                        <% leads.forEach(lead => { %>
                            <tr>
                                <td>
                                    <%= lead.createdAt ? new Date(lead.createdAt).toLocaleDateString('en-US', {
                                        year: 'numeric' , month: 'short' , day: 'numeric' }) : 'N/A' %>
                                </td>
                                <td>
                                    <% 
                                    let formTypeDisplay = lead.formType || 'Unknown';
                                    let badgeClass = 'bg-secondary';
                                    switch(lead.formType) {
                                        case 'contact': badgeClass = 'bg-primary'; break;
                                        case 'suggestion': badgeClass = 'bg-info'; break;
                                        case 'accessibility': badgeClass = 'bg-success'; break;
                                        case 'grievance': badgeClass = 'bg-warning'; break;
                                        case 'complaint': badgeClass = 'bg-danger'; break;
                                    }
                                    %>
                                    <span class="badge <%= badgeClass %> text-white px-2 py-1 rounded">
                                        <%= formTypeDisplay.charAt(0).toUpperCase() + formTypeDisplay.slice(1) %>
                                    </span>
                                </td>
                                <td>
                                    <%= lead.fullName || lead.name || lead.complainantName || 'N/A' %>
                                </td>
                                <td>
                                    <%= lead.email || lead.complainantEmail || 'N/A' %>
                                </td>
                                <td>
                                    <%= lead.phone || 'N/A' %>
                                </td>
                                <td>
                                    <% 
                                    let subject = 'N/A';
                                    if (lead.formType === 'contact') subject = lead.selectOne || lead.interestedIn || 'Contact Inquiry';
                                    else if (lead.formType === 'suggestion') subject = 'Suggestion Box';
                                    else if (lead.formType === 'accessibility') subject = 'Accessibility Feedback';
                                    else if (lead.formType === 'grievance') subject = 'Grievance Report';
                                    else if (lead.formType === 'complaint') subject = lead.complaintNature || 'Formal Complaint';
                                    else subject = lead.interestedIn || 'General Inquiry';
                                    %>
                                    <%= subject %>
                                </td>
                                <td>
                                    <div class="d-flex justify-content-center align-items-center">
                                        <button type="button"
                                            class="btn btn-sm btn-outline-success view-lead-btn"
                                            data-lead-id="<%= lead._id %>" data-lead-data='<%= JSON.stringify(lead) %>'>
                                            <iconify-icon icon="majesticons:eye-line" class="icon"></iconify-icon>
                                        </button>
                                    </div>
                                </td>
                            </tr>
                        <% }) %>
                    <% } else { %>
                        <tr>
                            <td colspan="7" class="text-center py-4">
                                <span class="text-secondary-light">
                                    <% if (currentFormType !== 'all') { %>
                                        No <%= currentFormType %> forms found
                                    <% } else { %>
                                        No leads found
                                    <% } %>
                                </span>
                            </td>
                        </tr>
                    <% } %>
                </tbody>
            </table>
        </div>

        <% if (pagination && pagination.totalItems > 0) { %>
        <div class="d-flex align-items-center justify-content-between flex-wrap gap-2 mt-4 pt-3 border-top">
            <div class="text-muted">
                Showing <%= ((pagination.currentPage - 1) * pagination.limit) + 1 %> to 
                <%= Math.min(pagination.currentPage * pagination.limit, pagination.totalItems) %> 
                of <%= pagination.totalItems %> entries
                <% if (currentFormType !== 'all') { %>
                    (<%= currentFormType %> forms)
                <% } %>
                <% if (search) { %>
                    for "<%= search %>"
                <% } %>
            </div>
            
            <% if (pagination.totalPages > 1) { %>
            <nav aria-label="Page navigation">
                <ul class="pagination d-flex flex-wrap align-items-center gap-2 justify-content-center mb-0">
                    <!-- Previous Button -->
                    <li class="page-item">
                        <% if (pagination.hasPrev) { %>
                            <a class="page-link bg-neutral-300 text-secondary-light fw-semibold radius-8 border-0 d-flex align-items-center justify-content-center h-32-px w-32-px text-md" 
                               href="?page=<%= pagination.prevPage %><%= currentFormType !== 'all' ? '&formType=' + currentFormType : '' %><%= search ? '&search=' + encodeURIComponent(search) : '' %>">
                                <iconify-icon icon="ep:d-arrow-left"></iconify-icon>
                            </a>
                        <% } else { %>
                            <span class="page-link bg-neutral-200 text-neutral-400 fw-semibold radius-8 border-0 d-flex align-items-center justify-content-center h-32-px w-32-px text-md">
                                <iconify-icon icon="ep:d-arrow-left"></iconify-icon>
                            </span>
                        <% } %>
                    </li>

                    <!-- Dynamic Page Numbers -->
                    <% 
                        let startPage = Math.max(1, pagination.currentPage - 2);
                        let endPage = Math.min(pagination.totalPages, pagination.currentPage + 2);
                        
                        // Adjust if we're near the beginning or end
                        if (endPage - startPage < 4) {
                            if (startPage === 1) {
                                endPage = Math.min(pagination.totalPages, startPage + 4);
                            } else {
                                startPage = Math.max(1, endPage - 4);
                            }
                        }
                    %>

                    <% if (startPage > 1) { %>
                        <li class="page-item">
                            <a class="page-link bg-neutral-300 text-secondary-light fw-semibold radius-8 border-0 d-flex align-items-center justify-content-center h-32-px w-32-px text-md" 
                               href="?page=1<%= currentFormType !== 'all' ? '&formType=' + currentFormType : '' %><%= search ? '&search=' + encodeURIComponent(search) : '' %>">1</a>
                        </li>
                        <% if (startPage > 2) { %>
                            <li class="page-item">
                                <span class="page-link bg-transparent text-secondary-light fw-semibold radius-8 border-0 d-flex align-items-center justify-content-center h-32-px w-32-px text-md">...</span>
                            </li>
                        <% } %>
                    <% } %>

                    <% for (let i = startPage; i <= endPage; i++) { %>
                        <li class="page-item">
                            <% if (i === pagination.currentPage) { %>
                                <span class="page-link text-white fw-semibold radius-8 border-0 d-flex align-items-center justify-content-center h-32-px w-32-px text-md bg-primary-600"><%= i %></span>
                            <% } else { %>
                                <a class="page-link bg-neutral-300 text-secondary-light fw-semibold radius-8 border-0 d-flex align-items-center justify-content-center h-32-px w-32-px text-md" 
                                   href="?page=<%= i %><%= currentFormType !== 'all' ? '&formType=' + currentFormType : '' %><%= search ? '&search=' + encodeURIComponent(search) : '' %>"><%= i %></a>
                            <% } %>
                        </li>
                    <% } %>

                    <% if (endPage < pagination.totalPages) { %>
                        <% if (endPage < pagination.totalPages - 1) { %>
                            <li class="page-item">
                                <span class="page-link bg-transparent text-secondary-light fw-semibold radius-8 border-0 d-flex align-items-center justify-content-center h-32-px w-32-px text-md">...</span>
                            </li>
                        <% } %>
                        <li class="page-item">
                            <a class="page-link bg-neutral-300 text-secondary-light fw-semibold radius-8 border-0 d-flex align-items-center justify-content-center h-32-px w-32-px text-md" 
                               href="?page=<%= pagination.totalPages %><%= currentFormType !== 'all' ? '&formType=' + currentFormType : '' %><%= search ? '&search=' + encodeURIComponent(search) : '' %>"><%= pagination.totalPages %></a>
                        </li>
                    <% } %>

                    <!-- Next Button -->
                    <li class="page-item">
                        <% if (pagination.hasNext) { %>
                            <a class="page-link bg-neutral-300 text-secondary-light fw-semibold radius-8 border-0 d-flex align-items-center justify-content-center h-32-px w-32-px text-md" 
                               href="?page=<%= pagination.nextPage %><%= currentFormType !== 'all' ? '&formType=' + currentFormType : '' %><%= search ? '&search=' + encodeURIComponent(search) : '' %>">
                                <iconify-icon icon="ep:d-arrow-right"></iconify-icon>
                            </a>
                        <% } else { %>
                            <span class="page-link bg-neutral-200 text-neutral-400 fw-semibold radius-8 border-0 d-flex align-items-center justify-content-center h-32-px w-32-px text-md">
                                <iconify-icon icon="ep:d-arrow-right"></iconify-icon>
                            </span>
                        <% } %>
                    </li>
                </ul>
            </nav>
            <% } %>
        </div>
        <% } else { %>
        <div class="d-flex align-items-center justify-content-center mt-4 pt-3 border-top">
            <div class="text-muted">No leads found</div>
        </div>
        <% } %>
    </div>
</div>

<!-- Lead Details Modal -->
